{
  "name": "user_modal",
  "type": "form",
  "container": [
    {
      "name": "_id",
      "type": "hidden"
    },
    {
      "name": "Username",
      "type": "input",
      "label": "账号",
      "props": {
        "span": 12,
        "disabled": "{{_.get(rootValue, 'IsBuiltIn') === true}}"
      }
    },
    {
      "name": "Password",
      "type": "password",
      "label": "密码",
      "props": {
        "span": 12
      }
    },
    {
      "name": "Name",
      "type": "input",
      "label": "姓名",
      "props": {
        "span": 12,
        "disabled": "{{_.get(rootValue, 'IsBuiltIn') === true}}"
      }
    },
    {
      "name": "Disable",
      "type": "switch",
      "label": "是否禁用",
      "props": {
        "span": 12
      }
    },
    {
      "condition": "{{!_.isEmpty(_.get(rootValue, 'ID'))}}",
      "name": "IsBuiltIn",
      "type": "switch",
      "label": "系统内置",
      "props": {
        "span": 24,
        "disabled": true
      }
    },
    {
      "condition": "{{!_.isEmpty(_.get(rootValue, 'ID'))}}",
      "name": "CreatedAt",
      "type": "text",
      "label": "创建时间",
      "props": {
        "span": 12
      }
    },
    {
      "condition": "{{!_.isEmpty(_.get(rootValue, 'ID'))}}",
      "name": "UpdatedAt",
      "type": "text",
      "label": "修改时间",
      "props": {
        "span": 12
      }
    }
  ]
}
